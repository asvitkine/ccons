#!/usr/bin/expect -f
log_user 0
set timeout 1

proc check {input output} {
    send "$input\n"
    expect timeout {
	send_user "Failed: input \"$input\" did not result in \"$output\" \n"
	exit
    } "$output"
}

spawn ../../ccons
send "unsigned j;\n"
send "int i = 0;\n"
check "i = 5;"        "=> (int) 5"
check "i*i;"          "=> (int) 25"
check "i-1;"          "=> (int) 4"
check "i+=i<<2;"      "=> (int) 25"
check "i - 100;"      "=> (int) -75"
check "j = 0-1;"      "=> (unsigned int) 4294967295"
